@model Plumber.Models.NotYetArrivedModel

@section Meta{
    <title>Heating Air Conditioning, Emergency Plumbing Repair Service, Drain Cleaning</title>
    <meta name="description" content="1-800-PLUMBER offers 24 hour emergency plumbing services, electrical repair, drain cleaning and heating air conditioning repairing service in United States and Canada." />
    <meta name="keywords" content="plumbing services,heating air conditioning,emergency plumbing service,plumbing repair,drain cleaning,24 hour plumbing service,electrical repair" />
}
<table width="100%" border="0" cellspacing="0" cellpadding="0">
    <tr>
        <td align="left" valign="top" style="height: 400px">
            @if (Request.QueryString["error"].ToString() == "1")
                {
                <div class="rowtitle">
                    Sorry. 1-800 PLUMBER has not yet arrived in your area.
                </div>
                }
                else if (Request.QueryString["error"].ToString() == "2")
                {
                <div class="rowtitle">
                    Sorry, that service is not yet offered in your area.
                </div>
                }
            <div>
                <div class="maincontent">
                    <p>
                        We are rapidly expanding! To be notified when our services are available in your
                        area and to be added to our contact list to receive special offers, please fill
                        in the form below.</p>
                    <p>
                        For information on Franchise opportunities in this area, @Html.ActionLink("click here", "Index", "TerritoryOwners").
                    </p>
                </div>
                @using (Html.BeginForm())
                {
                <table width="80%" border="0" align="center" cellpadding="0" cellspacing="0" class="form"
                    style="margin: auto;" cellpading="0">
                    <tr class="required">
                        <th width="31%" height="30" align="left" valign="middle" class="forminfo">
                            <label for="ZipCode">
                                ZIP *</label>
                        </th>
                        <td width="69%" height="30" align="left" valign="middle">
                            @*<input type="text" id="ZipCode" value="@ViewBag.ZipCode" class="formtext" />*@
                            @Html.TextBoxFor(model => model.ZipCode, new { @class = "formtext" })
                        </td>
                    </tr>
                    <tr class="required">
                        <th width="31%" height="30" align="left" valign="middle" class="forminfo">
                            <label for="Name">
                                First Last Name *</label>
                        </th>
                        <td width="69%" height="30" align="left" valign="middle">
                            @Html.TextBoxFor(model => model.Name, new { @class = "formtext" })
                        </td>
                    </tr>
                    <tr class="required">
                        <th width="31%" height="30" align="left" valign="middle" class="forminfo">
                            <label for="Email">
                                Email *</label>
                        </th>
                        <td width="69%" height="30" align="left" valign="middle">
                            @Html.TextBoxFor(model => model.Email, new { @class = "formtext" })
                        </td>
                    </tr>
                    <tr>
                        <td align="left" valign="middle" class="forminfo">
                        </td>
                        <td height="30" align="left" valign="middle">
                            <input class="btn_primary" name="submit" type="submit" value="Submit" />
                        </td>
                    </tr>
                </table>
                }
            </div>
        </td>
    </tr>
</table>
@section HeaderImage{
    <div class="aboutus">
        <img src="@Url.Content("~/Content/images/location-new.jpg")"
                alt="Locations" width="1004" height="187" /></div>
}
@section Navigation{
    <div class="navigation">
        <div class="menu bubplastic horizontal orange">
            <ul>
                <li><span class="menu_r"><a href="@Url.Content("~/")" title="1-800-PLUMBER"><span class="menu_ar">home</span></a></span></li>
                <li><span class="menu_r"><a href="@Url.Content("~/Locations")" title="Plumbing Services Locations"><span class="menu_ar">
                    Locations</span></a></span></li>
                <li><span class="menu_r"><a href="@Url.Content("~/Services")" title="Plumbing and Drain CleaningServices"><span class="menu_ar">
                    Services</span></a></span></li>
                <li><span class="menu_r"><a href="@Url.Content("~/AboutUs")" title=" About 1-800-PLUMBER"><span class="menu_ar">About
                    us </span></a></span></li>
                <li><span class="menu_r"><a href="@Url.Content("~/TerritoryOwners")"><span class="menu_ar">
                    Territory Owners</span></a></span></li>
                <li><span class="menu_r"><a href="@Url.Content("~/ContactUs")" title="Contact for Plumbing and Heating air conditioning"><span class="menu_ar">
                    Contact Us</span></a></span></li>
                @if (Request.IsAuthenticated)
                {
                    <li><span class="menu_r"><a href="@Url.Content("~/LandingPage/Dashboard")" title="1-800-PLUMBER Dashboard"><span class="menu_ar">
                        Dashboard</span></a></span></li>
                    <li><span class="menu_r"><a href="@Url.Content("~/SGAccount/LogOff")"><span class="menu_ar">
                        Log Off</span></a></span></li>
                }
                else
                {
                    <li><span class="menu_r"><a href="@Url.Content("~/SGAccount/LogOn")"><span class="menu_ar">
                        Log On</span></a></span></li>
                }
            </ul>
            <br class="clearit" />
        </div>
    </div>
}
<script type="text/javascript">

    $(document).ready(function () {
        $("#Name").focus();
        $("#ZipCode").mask("99999");
    });

</script>
