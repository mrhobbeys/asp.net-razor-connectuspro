@{
    ViewBag.Title = "Home Page";
}
@section headDeclaration{
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.cycle.all.2.74.js")"></script>
}
@section Meta{
    <title>Heating Air Conditioning, Emergency Plumbing Repair Service, Drain Cleaning</title>
    <meta name="description" content="1-800-PLUMBER offers 24 hour emergency plumbing services, electrical repair, drain cleaning and heating air conditioning repairing service in United States and Canada." />
    <meta name="keywords" content="plumbing services,heating air conditioning,emergency plumbing service,plumbing repair,drain cleaning,24 hour plumbing service,electrical repair" />
}
<style>
    .backstyle
    {
        background-color: #FFF;
    }
</style>
<script type="text/javascript">
    $(document).ready(function () {
        $("#plumbing").cycle({
            fx: "fade",
            timeout: 8000,
            delay: 2000,
            speed: 4000
        });
//        $("#plumbing").click(function () {
//            window.location.replace('@Url.Content("~/Plumbing")');
//        });

        $("#hvac").cycle({
            fx: "fade",
            timeout: 7000,
            delay: 1000,
            speed: 4000
        });
//        $("#hvac").click(function () {
//            window.location.replace('@Url.Content("~/Hvac")');
//        });

        $("#electrical").cycle({
            fx: "fade",
            timeout: 7500,
            delay: 1500,
            speed: 4000
        });
//        $("#electrical").click(function () {
//            window.location.replace('@Url.Content("~/Electrical")');
//        });

    });
</script>
