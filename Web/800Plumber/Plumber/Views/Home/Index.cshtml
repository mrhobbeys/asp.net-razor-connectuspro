                            <div class="col-middle-content">
                                <img src="@Url.Content("~/Content/images/our-guarantee.png")" alt="Our Guarantee" width="211" height="36" border="0" /></div>
                            <div class="col-middle-image">
                                <img src="@Url.Content("~/Content/images/gurantee.gif")" alt="Our Guarantee" width="204" border="0" /></div>
