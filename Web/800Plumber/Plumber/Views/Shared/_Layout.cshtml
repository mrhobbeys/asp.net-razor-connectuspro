<!DOCTYPE html>
<html>
<head>
    @*<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />*@
    @RenderSection("Meta", false)
    <link href="@Url.Content("~/Content/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/menu_style.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput-1.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.watermark.js")" type="text/javascript"></script>
    @RenderSection("headDeclaration", false)
    <style type="text/css">
        @*.style1 {font-family: Geneva, Arial, Helvetica, sans-serif}*@
    </style>
</head>
<body>
    <div id="wrapper">
        @Html.Partial("Header")
        @RenderSection("Navigation", false)
        @*@Html.Partial("Navigation")*@
        @RenderSection("HeaderImage", false)
        <div class="container">
            @RenderBody()
        </div>
        @Html.Partial("Footer")
    </div>
    <div align="center">
    </div>
</body>
</html>


<script type="text/javascript">
    $(document).ready(function () {
        $("#section1").fadeIn(2000);
        $("#section1").animate({ left: "10px" }, 2000);
        $("#section1").fadeIn(2000);
    });
</script>