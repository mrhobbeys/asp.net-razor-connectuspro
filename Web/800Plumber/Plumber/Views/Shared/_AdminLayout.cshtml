<!DOCTYPE html>
<html>
<head>
    @*<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />*@ <title>
        Plumbers and Plumbing Repair Services | Admin Section</title>
    <link href="@Url.Content("~/Content/admin.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/menu_style.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/tiny_mce/tiny_mce.js")" type="text/javascript"></script>
    @RenderSection("Scripts", false)
    @*<style type="text/css">
        .style1 {font-family: Geneva, Arial, Helvetica, sans-serif}
    </style>*@
</head>
<body>
    <div id="wrapper">
        <div class="top-row">
            <div class="logo">
                <a href="@Url.Content("~/Admin/Dashboard")">
                    <img src="@Url.Content("~/Content/images/1800-plumber.jpg")"
                alt="1800-plumber" width="285" height="54" border="0" /></a></div>
            <div class="animtext" style="padding-top: 20px"><label style="font-size: 20pt; font-weight: bold; font-style:italic">Administration Panel</label></div>
            <div class="call" style="padding-top: 54px">
                @Html.ActionLink("Main site", "Index", "Home", new { area = "" }, new { @target = "_blank" })
                | @Html.ActionLink("My Dashboard", "Index", "Dashboard", new { area = "Admin" }, null) |
                @Html.Partial("_LogOnPartial")</div>
        </div>
        <div class="navigation">
            <div class="menu bubplastic horizontal orange">
                <ul>
                    @*<li><span class="menu_r"><a href="@Url.Content("~/Admin/Franchise")"><span class="menu_ar">Franchises</span></a></span></li>*@
                    <li><span class="menu_r"><a href="@Url.Content("~/Admin/Location")"><span class="menu_ar">Locations</span></a></span></li>
                    <li><span class="menu_r"><a href="@Url.Content("~/Admin/Service")"><span class="menu_ar">
                        Services</span></a></span></li>
                    <li><span class="menu_r"><a href="@Url.Content("~/Admin/Management")"><span class="menu_ar">
                        Management</span></a></span></li>
                    <li><span class="menu_r"><a href="@Url.Content("~/Admin/TerritoryOwner")"><span class="menu_ar">
                        T. Owners</span></a></span></li>
                    <li><span class="menu_r"><a href="@Url.Content("~/Admin/Testimonial")"><span class="menu_ar">
                        Testimonial</span></a></span></li>
                    <li><span class="menu_r"><a href="@Url.Content("~/Admin/Media")"><span class="menu_ar">Media</span></a></span></li>
                    <li><span class="menu_r"><a href="@Url.Content("~/Admin/Coupon")"><span class="menu_ar">
                        Coupons</span></a></span></li>
                    <li><span class="menu_r"><a href="@Url.Content("~/Admin/Career")"><span class="menu_ar">
                        Careers</span></a></span></li>
                </ul>
                <br class="clearit" />
            </div>
        </div>
        <div class="container">
            @RenderBody()
        </div>
        <div id="footer">
            <a href="@Url.Content("~/Privacy")" class="emaillink">Click here</a> for Legal &amp;
            Privacy Policies. 1-800-PLUMBER&reg; 2010. All Rights Reserved by 1-800-PLUMBER&reg;
            and www.1800plumber.com<br />
        </div>
    </div>
    <div align="center">
    </div>
</body>
</html>
<script type="text/javascript">
    $(document).ready(function () {
        $("#section1").fadeIn(2000);
        $("#section1").animate({ left: "10px" }, 2000);
        $("#section1").fadeIn(2000);
    });
</script>