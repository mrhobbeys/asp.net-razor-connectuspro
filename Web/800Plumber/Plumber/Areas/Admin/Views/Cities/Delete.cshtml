@model Plumber.Models.CityServed
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>
    confirm delete</h2>
<h3>
    Are you sure you want to delete the city @Model.City for the location @Model.Location.LocationName?</h3>
@using (Html.BeginForm())
{
    <p>
        <input type="submit" value="Delete" />
        |
        @Html.ActionLink("Back to List", "Index", new { id = Model.LocationId })
    </p>
}
