@model      SiteBlue.Areas.HVAC_App.Models.SetupModels.GuaranteeModel[]
<script>
    var tabbarGuarantees;
    $(document).ready(function() {
        tabbarGuarantees = new dhtmlXTabBar("tabGurantees", "top");
        tabbarGuarantees.setSkin('dhx_skyblue');
        tabbarGuarantees.setImagePath("../../../../codebase/imgs/");
        tabbarGuarantees.setHrefMode("ajax-html");
        @{
            for (int i = 0; i < Model.Length; ++i)
            {
                <text>
                    tabbarGuarantees.addTab("a" + @i, "@Model[i].GuaranteeName", "170px");      
                    tabbarGuarantees.setContentHref("a" + @i, "@Url.Action("GetGuarantee", Model[i])".replace("&amp;","&"));
                </text>
            }
           
        }
        tabbarGuarantees.enableAutoReSize(true);
        tabbarGuarantees.setTabActive("a0");
    });
</script>       
<div id="tabGurantees" style="width: 100%; height: 100%;"></div>