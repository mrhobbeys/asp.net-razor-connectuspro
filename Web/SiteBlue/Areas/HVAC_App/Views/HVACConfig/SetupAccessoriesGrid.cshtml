
<script>
    var grid_acc;
    $(document).ready(function() {
        grid_acc = new dhtmlXGridObject('grid_acc');
        grid_acc.setImagePath("../../../../codebase/imgs/");
        grid_acc.setHeader("Code,Accessory Name,Price,UE,SE,HE,ME,LE,Linked Question", null, ["text-align:left;", "text-align:center;", "text-align:center", "text-align:center", "text-align:center", "text-align:center", "text-align:center"]);
        grid_acc.setColumnIds("cd,AccDescrptn,pr,ue,se,he,me,le,qw");
       // grid_acc.setColumnHiddenue(1, true);
//        grid_acc.setColumnHidden(3, true);
        grid_acc.setInitWidths("80,*,60,50,50,50,50,50,200");
        grid_acc.setColTypes("ro,ro,ro,ch,ch,ch,ch,ch,coro");
        grid_acc.setColAlign("left,left,center,center,center,center,center,center, center");
        grid_acc.getCombo(8).put("0", "No Linked Question");
        grid_acc.getCombo(8).put("7", "Air Cleaner Question");
        grid_acc.getCombo(8).put("8", "UltraPure Lights Question");
        grid_acc.getCombo(8).put("9", "Humidifier Question");
        grid_acc.getCombo(8).put("34", "Duct Work Insulation Question");
        grid_acc.setSkin("light");
        grid_acc.enableAutoHeight(true, 500);
        grid_acc.init();
        grid_acc.load("../../../Hvac_app/hvacconfig/GetAllAccessories", "json");
        dp = new dataProcessor("../../../Hvac_app/hvacconfig/SaveAccessoriesPackage");
        dp.setUpdateMode("off");
        dp.enableDataNames(true);
        dp.setTransactionMode("POST");
        dp.init(grid_acc);
    });

</script>
<div id="AccGrid" style="width:99.8%;height:99.6%;">
            <div id="grid_acc" style="width:100%;height:150px; overflow-y: hidden; overflow-x: hidden;"></div> 
            <button type="button" id="btn_save_acc" onclick="SaveSelection()" >Save</button>
</div>