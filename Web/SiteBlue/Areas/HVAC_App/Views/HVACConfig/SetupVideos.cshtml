@model      SiteBlue.Areas.HVAC_App.Models.SetupModels.PackageVideoCode[]
<script>
    var tabbarVideos;
    $(document).ready(function() {
        tabbarVideos = new dhtmlXTabBar("tabVideos", "top");
        tabbarVideos.setSkin('dhx_skyblue');
        tabbarVideos.setImagePath("../../../../codebase/imgs/");
        tabbarVideos.setHrefMode("ajax-html");
        @{
            for (int i = 0; i < Model.Length; ++i)
            {
                <text>
                    tabbarVideos.addTab("a" + @i, "@Model[i].PackageName Video", "60px");      
                    tabbarVideos.setContentHref("a" + @i, "@Url.Action("GetVideo", Model[i])".replace("&amp;","&"));
                </text>
            }
        }
        tabbarVideos.setTabActive("a0");
    });
</script>       

<div id="tabVideos" style="width: 100%; height: 100%;"></div>