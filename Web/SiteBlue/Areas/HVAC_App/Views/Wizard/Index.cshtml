@{
    Layout = null;
}

<!DOCTYPE html>

<html>
    <head>
        @*        <meta name="apple-touch-fullscreen" content="yes" />
        <meta name="apple-mobile-web-app-capable" content="yes" />*@
        <meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate"/>
        <meta http-equiv="Pragma" content="no-cache"/>
        <meta http-equiv="Expires" content="0"/>
        <meta name="viewport" content="width=device-width; initial-scale=1.0; maximum-scale=1.0; user-scalable=0;" />  

        <title>HVAC App</title> 

        <link  rel="apple-touch-icon"  href="@Url.Content("~/Areas/HVAC_App/Content/apple-touch-icon.png")" /> 
         
        <script src="http://maps.google.com/maps/api/js?sensor=false" type="text/javascript"></script> 
        <script type="text/javascript" src="https://www.google.com/jsapi"></script>

        <link rel="stylesheet" href="@Url.Content("~/Areas/HVAC_App/Scripts/codebase/touichui.css")" type="text/css" media="screen" charset="utf-8"/>
        <link rel="stylesheet" href="@Url.Content("~/Areas/HVAC_App/content/dhtmlx_custom.css")" type="text/css" media="screen" charset="utf-8"/>
        <link rel="stylesheet" href="@Url.Content("~/Areas/HVAC_App/content/site.css")" type="text/css" media="screen" charset="utf-8"/>

        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/Data.js")" type="text/javascript" charset="utf-8"></script>  
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/codebase/touchui.js")" type="text/javascript" charset="utf-8"></script>
         
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/main.js")" type="text/javascript" charset="utf-8"></script> 
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/Hvac_questions.js")" type="text/javascript" charset="utf-8"></script> 
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/setAnswers.js")" type="text/javascript" charset="utf-8"></script>  
        
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/Hvac_Login.js")" type="text/javascript" charset="utf-8"></script>  
       
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/Hvac_Preparation.js")" type="text/javascript" charset="utf-8"></script>  
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/content.js")" type="text/javascript" charset="utf-8"></script>  

        <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript" charset="utf-8"></script>
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/globalize.js")" type="text/javascript" charset="utf-8"></script>
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/globalize.culture.en-US.js")" type="text/javascript" charset="utf-8"></script>
        <script src="@Url.Content("~/Areas/HVAC_App/Scripts/jSignature.js")" type="text/javascript" charset="utf-8"></script>
        
    </head>
    <body>
        <div id="videoTemplate" style="display: none;" >
            <div>
               <object>
                    <param name="movie" value="http://www.youtube.com/v/#url#?version=3&feature=player_detailpage">
                    <param name="allowFullScreen" value="true">
                    <param name="allowScriptAccess" value="false">        
                    <embed src="http://www.youtube.com/v/#url#?version=3&feature=player_detailpage" type="application/x-shockwave-flash" allowfullscreen="true" allowScriptAccess="always" width="435" height="260">
               </object>         
            </div>
        </div>
        <div id="signTemplate" style="display: none;"> 
            <div id="sign_1"></div>      
        </div> 
        <div id="priceListTemplate" style="display: none;">
            <div class="dhx_el_label" style="width:745px; text-align:center; {common.showMainRow()}">
                <div style="float: left; width: 86px;">&nbsp</div>
                <div style="float: left; width: 50px;">#Count#</div>
                <textarea rows="2" contenteditable="true" onchange="view35.ChangeDesriptionOnMain(#id#, this)" class="dhx_view box" style="float: left; font-size: 22px; color: #474747;  width: 450px;">#Description#</textarea>
                <div style="float: left; width: 20px; overflow-x: hidden; color:white;">#Price#</div>
                <div style="float: left; width: 20px; color:white;">#TotalPrice#</div>
            </div>      
            <div class="dhx_el_label" style="width:745px; {common.showMainPartRow()}">
                <div style="float: left; width: 86px;"><button type="button" id="btn_#id#" onclick="view35.removePart('#id#')"  class="dhx_inp_form_button" style="float: left;">delete</button></div>
                <div style="float: left; width: 50px;">#Qty#</div>
                <div style="float: left; width: 450px;">
                    <button type="button" id="btn_#id#add" onclick="view35.addPart('#id#')"  class="dhx_inp_form_button" style="float: left;">Add</button>
                    <div style="float: left; width: 70px; ">PART</div>
                    #PartName#
                </div>
                <div style="float: left; width: 80px; color:white;">#Price#</div>
                <div style="float: left; width: 70px; color:white;">#TotalPrice#</div>
            </div>               
            <div class="dhx_el_label" style="width:745px;  {common.showAccessoryRow()}">
                <div style="float: left; width: 86px;"><button type="button" id="btn_#id#" onclick="view35.removeItem('#id#')"  class="dhx_inp_form_button" style="float: left;">delete</button></div>
                <div style="float: left; width: 50px;">#Count#</div>
                <div style="float: left; width: 450px;">#Description#</div>
                <div style="float: left; width: 80px; color:white;">#Price#</div>
                <div style="float: left; width: 70px; color:white;">#TotalPrice#</div>
            </div>               
        </div>
        <div id="priceListHeaderTemplate" style="display: none;">
             <div class="dhx_el_label" style="width:745px; text-align:center;">
                <div style="float: left; width: 86px;">&nbsp</div>
                <div style="float: left; width: 50px;">#count#</div>
                <div style="float: left; width: 450px;">#description#</div>
                <div style="float: left; width: 80px; color:white;">#price#</div>
                <div style="float: left; width: 70px; color:white;">#total_price#</div>
            </div>               
        </div>
        <div id="paymentItem" style="display: none;">
            <div class="dhx_el_label" style="width:745px;">
                <button type="button" id="btn_#id#" onclick="removePayment(#id#)"  class="dhx_inp_form_button" style="float: left;">delete</button>
                <div style="float: left; width: 250px;">#type#</div>
                <div style="float: left; width: 150px;">#payment#</div>
                <div style="float: left; width: 150px;">#code#</div>
            </div>
        </div>
        <div id="PictureTemplate" style="display: none;">
            <div class="picture" style="background-image: url('#urlText#')">&nbsp</div>
        </div>
    </body>
</html>