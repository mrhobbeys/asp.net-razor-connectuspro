<body style="margin:0px;">

@Html.Partial("CompanyLogoHeader", Model)
<div id="a_tabbar" class="dhtmlxTabBar" imgpath="../../../../codebase/imgs/" style="width: 100%;
height: 100%;">
<div id="a1" name="Name and Address">
<div style="float: left; padding-left: 20px; padding-top: 15px;">
<span style="font-family: 18px; font-weight: bold;">Customer Name and Address</span>
&nbsp;&nbsp;&nbsp;&nbsp; <input id="btnRTExcel" type="button" onclick="downloadBankingDepositesToExcel()"  value="Export to Excel" />
</div>

<div style="clear: both; height: 350px; float: left; padding-left: 20px;
padding-top: 15px;">
<div id="gridbox" style="width: 100%; height: 350px; background-color: white; overflow: hidden">
</div>
</div>

<script type="text/javascript" language="javascript" >
    mygrid = new dhtmlXGridObject('gridbox');
    mygrid.setImagePath("../../../../codebase/imgs/");
    mygrid.setHeader("Customer,#cspan,#cspan,Address,#cspan,#cspan,#cspan", null, ["text-align:center;", "text-align:center;"]);
    mygrid.setInitWidths("200,200,200,150,150,150,150");
    mygrid.setColAlign("right,left,left,right,center,left,center,center");
    mygrid.setColTypes("ed,ed,ed,ed,ed,co,ed,ed");
    mygrid.setColSorting("int,str,str,int,str,str,str,str");
    mygrid.setColumnColor("white,white,white,#d5f1ff,#d5f1ff,#d5f1ff,#d5f1ff");
    mygrid.attachHeader(["AccountNumber", "Customer", "Company Name", "Address", "State", "City", "Email"]); mygrid.setSizes();
    mygrid.setColumnMinWidth(50, 0);
    mygrid.init();
    mygrid.setSkin("dhx_skyblue");
    mygrid.enableMultiselect(true);
    mygrid.loadXML("../../../../common/gridCustomers.xml");

    function downloadBankingDepositesToExcel() {
     
    
        mygrid.toExcel("../Customer/DownloadMyFinances");
    }
</script>
</div>


