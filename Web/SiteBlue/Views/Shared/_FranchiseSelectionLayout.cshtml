<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
<head>
    @RenderSection("MetaTags", false)
    <title>@ViewBag.Title</title>
    <link href="/Content/SecurityGuard/securityguard.css" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/companyheader.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Content/SecurityGuard/scripts/jquery-1.6.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/companyheader.js")" type="text/javascript"></script>

    @RenderSection("ScriptIncludes", false)
    @RenderSection("InlineScript", false)
    @RenderSection("Styles", false)
    
</head>
<body class="inner_body">
    <div id="wrapper_inner" class='wrapper_inner'>
        <div id="header">
            @{Html.RenderAction("FranchiseSelectionHeader");}
        </div>
        <div id="content">
            <div class="innerpage">
                @RenderBody()
            </div>
        </div>
    </div>
</body>
</html>
