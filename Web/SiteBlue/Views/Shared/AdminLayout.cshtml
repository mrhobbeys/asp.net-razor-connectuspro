@{
    Layout = null;
    var swap = ViewBag.SwapBranding ?? false;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="content-type" content="text/html;charset=utf-8" />
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/base/reset.css")" type="text/css" media="screen" />
    <link rel="stylesheet" href="@Url.Content("~/Content/themes/base/master.css")" type="text/css" media="screen" />
    <!-- Include the Cufon files -->
    <script src="@Url.Content("~/Scripts/cufon.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Aller_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/tiny_mce/tiny_mce.js")" type="text/javascript"></script>
    <!-- CSS Browser Compatibility -->
    <script src="@Url.Content("~/Scripts/css_browser_select.js")" type="text/javascript"></script>
    <!-- Initialize Cufon -->
    <script type="text/javascript">
        Cufon.replace('h1, h2, h3');
        Cufon.replace('ul#nav li a', { hover: true });
        Cufon.replace('ul#navLogin li a', { hover: true });
        Cufon.replace('ul#navSidebar li a', { hover: true });
        Cufon.replace('ul#navFooter li a', { hover: true });
        Cufon.replace('.selected', { hover: true });

        Cufon.replace('blockquote p');
		
		
    </script>
    <!-- Include jQuery  -->
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.2/jquery.min.js" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/myfunctions.js")" type="text/javascript"></script>
    @RenderSection("Scripts", false)
    @RenderSection("Styles", false)
</head>
<body>
    <!-- Logo & Contact Info -->
    <div id="topContainer">
        @Html.Partial("_LandingTop")
        <!-- end centerContainer -->
    </div>
    <!-- end topContainer -->
    <!-- Menu Container -->
    @Html.Partial("_LandingMenu")
    <!-- end menuContainer -->
    <!-- Content -->
    <div id="Content" class="centerContainer">
        <!-- Left: SIDEBAR MENU -->
         @*@Html.Partial("_SideBarMenu")*@
        <!-- end Left: SIDEBAR MENU -->
        @*<div id="rightBar" class="fr">
        </div>*@
        <!--  4 pixel background in right part of content-->
        <!-- Right: BIG ICONS + DESCRIPTION -->
        @RenderBody()
        @RenderSection("BackSection", false)
        <!-- end Right: BIG ICONS + DESCRIPTION -->
    </div>
    <!-- end Content -->
    <!-- Footer Container-->
    <div id="footerBG">
        <!-- div for colored image-->
        @Html.Partial("_LandingFooter")
        <!-- end div pattern -->
    </div>
    <!-- end div background image -->
    <!-- End Footer Container -->
    <!-- Initialize Cufon -->
    <script type="text/javascript">
        Cufon.now();
    </script>
</body>
</html>
