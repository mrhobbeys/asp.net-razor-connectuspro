@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
<head>
    <title>ConnectUsPro</title>
    @*<meta http-equiv="X-UA-Compatible" content="IE=8" />*@
    <link href="@Url.Content("~/Content/template.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cufon-yui.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cufon-replace.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Swis721_LtCn_BT_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Swis721_Md_BT_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Swis721_Th_BT_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Vetren_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.cycle.all.2.74.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MicrosoftAjax.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MicrosoftMvcAjax.js")" type="text/javascript"></script>
    @RenderSection("scripts", false)
    <style>
        #logo
        {
            padding: 20px 0 0 0;
        }
        header
        {
        }
    </style>
</head>
<body id="hp">
    <header id="header" class="box">
        <p id="logo">
            <a href="@Url.Content("~/")" title="ConnectUsPro Home page">
                <img src="@Url.Content("~/Content/Images/new/logo.png")" alt="" /></a></p>
        <nav id="nav">
            <ul id="navigation">
                <li>@Html.ActionLink("Who we are", "Index", "Who", new { area = "" }, new { @class = (ViewBag.Current == "Who" ? "current" : "") })</li>
                <li>@Html.ActionLink("What we do", "Index", "What", new { area = "" }, new { @class = (ViewBag.Current == "What" ? "current" : "") })</li>
                <li>@Html.ActionLink("How we do it", "Index", "How", new { area = "" }, new { @class = (ViewBag.Current == "How" ? "current" : "") })</li>
                <li>@Html.ActionLink("Contact", "Index", "Contact", new { area = "" }, new { @class = (ViewBag.Current == "Contact" ? "current" : "") })</li>
                @if (User.Identity.IsAuthenticated)
                {
                    @*<li>@Html.ActionLink("Log Off", "LogOff", "SGAccount", new { area = "" }, null)</li>*@
                    <li>@Html.ActionLink("Dashboard", "Index", "Dashboard", new { area = "LandingPage" }, null)</li>
                }
                else
                {
                    <li>@Html.ActionLink("Log On", "LogOn", "SGAccount", new { area = "" }, new { @class = (ViewBag.Current == "LogOn" ? "current" : "") })</li>
                }
            </ul>
        </nav>
        @RenderSection("Comments", false)
        <div id="pagetitle">@RenderSection("Title", false)</div>
    </header>
    <div id="main">
        @RenderBody()
    </div>
    <footer id="footer">
        <div id="doffice">
            Office:<br />
            <strong style="font-family: Calibri, Tahoma; font-size: 14pt;">281-646-6641</strong>
        </div>
        <div id="demail">
            Email:<br />
            <a href="mailto:info@@connectuspro.com">info@@connectuspro.com</a>
        </div>
        <div id="dcontact">
            Are you a leader?<br />
            @Html.ActionLink("Contact us.", "Index", "Contact", new { area = "" }, null)
        </div>
    </footer>
</body>
</html>
