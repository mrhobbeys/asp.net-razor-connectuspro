@using SiteBlue.Models
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/themes/base/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/layout.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cufon-yui.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cufon-replace.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Swis721_LtCn_BT_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Swis721_Md_BT_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Swis721_Th_BT_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Vetren_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.cycle.all.2.74.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MicrosoftAjax.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MicrosoftMvcAjax.js")" type="text/javascript"></script>
    <style type="text/css">
        #slideshow
        {
            position: relative;
            width: 430px;
            margin: 35px;
        }
        #controls
        {
            z-index: 1000;
            position: absolute;
            top: 520px;
            left: 0;
            display: none;
            background-color: #ffffff;
            border: 0px solid #ddd;
            margin: 0;
            padding: 6px;
            width: 960px;
            zoom: 1;
            filter: alpha(opacity=70);
            opacity: 0.7;
        }
        #tagLine
        {
            z-index: 1000;
            position: absolute;
            top: 330px;
            left: 20px;
            background-color: transparent;
            border: 0px solid #ddd;
            margin: 0;
            padding: 6px;
            width: 418px;
            font: 35px/1.2em Arial, Helvetica, sans-serif;
            text-align: left;
        }
        #controls span
        {
            margin: 0 5px;
        }
    </style>
    <!--[if lt IE 9]><script src="@Url.Content("~/Scripts/ie9.js")" type="text/javascript"></script><![endif]-->
    <!--[if lt IE 7]><link href="@Url.Content("~/Content/themes/base/ie_style.css")" rel="stylesheet" type="text/css" /><![endif]-->
</head>
<body id="page2">
    <div class="footer-border">
        <div id="main">
            <!-- header -->
            <header>
                <h1>
                    @Html.ActionLink(".", "Index", "Home")</h1>
                @if (IsSectionDefined("navigation"))
                {      @RenderSection("navigation") }
                else { }
            </header><div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>
            @RenderBody()
            <!-- footer -->
            @Html.Partial("_footer")
        </div>
    </div>
</body>
</html>
